FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volVectorField;
    object          U;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    inlet           
    {
        type            fixedValue;
        value           uniform (1 0.0 0.0);
    }

    outlet          
    {
        type            zeroGradient;
    }

    walls       
    {
        type            fixedValue;
        value           uniform (0.0 0.0 0.0);
    }

}


